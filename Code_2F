library(ggplot2)

# Create the bar plot
ggplot(CD4merge, aes(x = sharing, fill = sharing)) +
  geom_bar() +
  facet_wrap(~chain) +
  labs(x = "Sharing Status", y = "Count", title = "Distribution of Public and Private by Chain Type") +
  theme_minimal() +
  theme(legend.position = "none")

